<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.superfly.cms.dao.AdminDao">


    <select id="queryAdminByName" resultType="com.superfly.cms.entity.Admin">
		SELECT *
       from admin_t
		WHERE
		admin_name=#{adminName}
	</select>

</mapper>